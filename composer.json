{
  "name": "arraypress/wp-accept-language-utils",
  "description": "A lean WordPress utility for parsing and working with HTTP Accept-Language headers",
  "version": "1.0.0",
  "type": "library",
  "license": "GPL-2.0-or-later",
  "homepage": "https://github.com/arraypress/wp-accept-language-utils",
  "authors": [
    {
      "name": "David Sherlock",
      "homepage": "https://arraypress.com/"
    }
  ],
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "ArrayPress\\AcceptLanguage\\": "src/"
    },
    "files": [
      "src/Functions.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/arraypress/wp-accept-language-utils/issues",
    "source": "https://github.com/arraypress/wp-accept-language-utils"
  },
  "keywords": [
    "wordpress",
    "accept-language",
    "language-detection",
    "locale",
    "i18n",
    "internationalization",
    "http-headers",
    "arraypress"
  ]
}